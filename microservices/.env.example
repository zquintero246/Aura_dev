# Copy to .env and set values (optional)

# Weather provider (default: open-meteo). Alternatives: openweather (requires key)
WEATHER_API_PROVIDER=open-meteo

# OpenWeatherMap key (only if WEATHER_API_PROVIDER=openweather or as fallback)
OPENWEATHERMAP_KEY=

# Database (used when calling /api/dashboard?user_id=...)
APP_DB_DRIVER=postgres
DB_HOST=aura_postgres
DB_PORT=5432
DB_DATABASE=aura_main
DB_USERNAME=aura_user
DB_PASSWORD=aura_pass
